{
  "manifest_version": 2,
  "description": "sci-hub helper",
  "version": "2.1",
  "name": "sci-hub helper",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}